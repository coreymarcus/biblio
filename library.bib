issn = {15523098},
journal = {IEEE Transactions on Robotics},
keywords = {Active perception,large-scale mapping,machine perception,multi-fidelity representation},
number = {1},
pages = {248--260},
publisher = {IEEE},
title = {{Efficient, Multifidelity Perceptual Representations via Hierarchical Gaussian Mixture Models}},
volume = {35},
year = {2019}
}
